    // const [bracket, setBracket] = useState('')\n\n    const history = useHistory();\n\n    const handleSubmit = (e) => {\n        e.preventDefault();\n\n        history.push({\n            pathname: '/bracket',\n            state: { key: pollKey }\n        });\n    };\n\n    return (\n        <div className=\"EnterCode\">\n            <h1>Enter Code</h1>\n            <div className=\"six-box\">\n                <input\n                    className=\"input\"\n                    onChange={(e) => setPollKey(e.target.value)}\n                    name=\"code\"\n                    type=\"text\"\n                    placeholder=\"8 DIGIT CODE\"\n                    maxLength=\"8\"></input>\n            </div>\n            <div>\n                <a onClick={handleSubmit} href=\".\" className=\"waves-effect waves-light btn pink\">\n                    Vote!\n                </a>\n            </div>\n        </div>\n    );\n}\n\nexport default EnterCode;\n","/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/VoteSubmitted.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/About.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/FourOFour.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/Public.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/EditBracket.js",["78","79"],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/CreatePoll.js",["80","81"],"import React, { Component } from 'react';\nimport Step1 from './Step1';\nimport Step2 from './Step2';\nimport M from 'materialize-css';\nimport axios from 'axios';\nimport { Redirect } from 'react-router-dom';\n\nexport default class CreatePoll extends Component {\n    constructor(props) {\n        super(props);\n        // Set the initial input values\n        this.state = {\n            currentStep: 1, // Default is Step 1\n            duration: '',\n            options: '',\n            title: '',\n            display: '',\n            private: false,\n            list: [],\n            redirect: false\n        };\n\n        this.handleChange = this.handleChange.bind(this);\n        this._next = this._next.bind(this);\n        this._prev = this._prev.bind(this);\n    }\n\n    componentDidMount() {\n        M.AutoInit();\n    }\n\n    componentDidUpdate() {\n        M.AutoInit();\n    }\n\n    _next() {\n        let currentStep = this.state.currentStep;\n        currentStep = currentStep >= 2 ? 2 : currentStep + 1;\n        this.setState({\n            currentStep: currentStep\n        });\n    }\n\n    _prev() {\n        let currentStep = this.state.currentStep;\n        currentStep = currentStep <= 1 ? 1 : currentStep - 1;\n        this.setState({\n            currentStep: currentStep\n        });\n    }\n\n    handleChange(event) {\n        const { name, value } = event.target;\n        if (name === 'private' && value === 'on') {\n            this.setState({ private: true });\n        }\n        if (name.includes('option-')) {\n            let index = parseInt(name.split('-')[1]);\n            let list = this.state.list.slice(0, this.state.list.length);\n            if (list.length === index) {\n                list[index - 1] = value;\n            } else {\n                list.push(value);\n            }\n            this.setState({ list });\n        }\n        this.setState({\n            [name]: value\n        });\n    }\n\n    // Trigger an alert on form submission\n    handleSubmit = (event) => {\n        event.preventDefault();\n        let num_options = this.state.list.length;\n        const newBracket = {\n            duration: this.state.duration,\n            title: this.state.title,\n            end_display: this.state.display,\n            private: this.state.private,\n            options_list: this.state.list,\n            num_options: num_options\n        };\n        axios\n            .post(`${process.env.REACT_APP_SERVER_URL}/bracket/create`, newBracket)\n            .then((newBracket) => {\n                console.log(newBracket.data);\n                if (!newBracket.data.msg.includes('created')) {\n                    this.setState({ error: true, loading: false });\n                } else {\n                    this.setState({ redirect: true, loading: false });\n                }\n            })\n            .catch((err) => {\n                this.setState({ error: true, loading: false });\n            });\n    };\n\n    get previousButton() {\n        let currentStep = this.state.currentStep;\n        if (currentStep !== 1) {\n            return (\n                <a className=\"waves-effect waves-light btn pink\" onClick={this._prev}>\n                    Previous\n                </a>\n            );\n        }\n        return null;\n    }\n\n    get nextButton() {\n        let currentStep = this.state.currentStep;\n        if (currentStep < 2) {\n            return (\n                <a className=\"waves-effect waves-light btn pink\" onClick={this._next}>\n                    Next\n                </a>\n            );\n        }\n\n        return null;\n    }\n    render() {\n        M.AutoInit();\n        if (this.state.redirect === true) {\n            return <Redirect to=\"/finishedcreate\" />;\n        }\n        if (this.state.error) {\n            return <div style={{ color: 'red' }}>AND ERROR HAS OCCURED. PLEASE TRY AGAIN OR CONTACT SUPPORT.</div>;\n        }\n        return (\n            <React.Fragment>\n                <h1>Create a Bracket</h1>\n\n                <form onSubmit={this.handleSubmit}>\n                    <Step1\n                        currentStep={this.state.currentStep}\n                        handleChange={this.handleChange}\n                        duration={this.state.duration}\n                        options={this.state.options}\n                        title={this.state.title}\n                        display={this.state.display}\n                    />\n                    <Step2\n                        currentStep={this.state.currentStep}\n                        handleChange={this.handleChange}\n                        options={this.state.options}\n                        handleSubmit={this.handleSubmit}\n                    />\n                    {this.previousButton}\n                    {this.nextButton}\n                </form>\n            </React.Fragment>\n        );\n    }\n}\n","/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/Welcome.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/Filter.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/Step2.js",["82"],"import React, { Component } from 'react';\nimport './css/Step2.css';\n\nexport default class Step2 extends Component {\n    render() {\n        if (this.props.currentStep !== 2) {\n            // Prop: The current step\n            return null;\n        }\n        let optionNum = parseInt(this.props.options);\n\n        function makeInput(optionNumber, props) {\n            return (\n                <input\n                    type=\"text\"\n                    placeholder={'Option ' + optionNumber}\n                    name={'option-' + optionNumber}\n                    value={props.list}\n                    onChange={props.handleChange}\n                />\n            );\n        }\n        const matchups = [];\n\n        for (let i = 1; i < optionNum; i += 2) {\n            let option1 = makeInput(i, this.props);\n            let option2 = makeInput(i + 1, this.props);\n            let finalDiv = (\n                <div className=\"bracket-options\">\n                    {option1}\n                    <br></br>\n                    VS\n                    <br></br>\n                    {option2}\n                </div>\n            );\n            matchups.push(finalDiv);\n        }\n\n        return (\n            <div className=\"brackets\">\n                {matchups}\n                <a className=\"waves-effect waves-light btn pink\" onClick={this.props.handleSubmit} href=\"\">\n                    Submit\n                </a>\n            </div>\n        );\n    }\n}\n","/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/Step1.js",[],{"ruleId":"83","replacedBy":"84"},{"ruleId":"85","replacedBy":"86"},{"ruleId":"87","severity":1,"message":"88","line":26,"column":15,"nodeType":"89","messageId":"90","endLine":26,"endColumn":31},{"ruleId":"87","severity":1,"message":"91","line":2,"column":8,"nodeType":"89","messageId":"90","endLine":2,"endColumn":13},{"ruleId":"92","severity":1,"message":"93","line":152,"column":21,"nodeType":"94","endLine":152,"endColumn":121},{"ruleId":"92","severity":1,"message":"93","line":156,"column":21,"nodeType":"94","endLine":156,"endColumn":94},{"ruleId":"92","severity":1,"message":"93","line":103,"column":17,"nodeType":"94","endLine":103,"endColumn":83},{"ruleId":"92","severity":1,"message":"93","line":115,"column":17,"nodeType":"94","endLine":115,"endColumn":83},{"ruleId":"92","severity":1,"message":"95","line":43,"column":17,"nodeType":"94","endLine":43,"endColumn":104},"no-native-reassign",["96"],"no-negated-in-lhs",["97"],"no-unused-vars","'setCurrentSocket' is assigned a value but never used.","Identifier","unusedVar","'axios' is defined but never used.","jsx-a11y/anchor-is-valid","The href attribute is required for an anchor to be keyboard accessible. Provide a valid, navigable address as the href value. If you cannot provide an href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but stil