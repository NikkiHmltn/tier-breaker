.bind(this)\n      this._next = this._next.bind(this)\n      this._prev = this._prev.bind(this)\n    }\n    componentDidMount() {\n      M.AutoInit(); \n    }\n    _next() {\n      let currentStep = this.state.currentStep\n      // If the current step is 1 or 2, then add one on \"next\" button click\n      currentStep = currentStep >= 2? 3: currentStep + 1\n      this.setState({\n        currentStep: currentStep\n      })\n    }\n\n    _prev() {\n      let currentStep = this.state.currentStep\n      // If the current step is 2 or 3, then subtract one on \"previous\" button click\n      currentStep = currentStep <= 1? 1: currentStep - 1\n      this.setState({\n        currentStep: currentStep\n      })\n    }\n\n    // Use the submitted data to set the state\n    handleChange(event) {\n      console.log(event.target)\n      const {name, value} = event.target\n      console.log(name)\n      console.log(value)\n      console.log(this.state)\n      this.setState({\n        [name]: value\n      })    \n    }\n    \n    // Trigger an alert on form submission\n    handleSubmit = (event) => {\n      event.preventDefault()\n      const { duration, options, title, display } = this.state\n    }\n\n  componentDidUpdate() {\n    console.log(\"IN UPDATE\")\n    M.AutoInit(); \n  }\n\nget previousButton(){\n  let currentStep = this.state.currentStep;\n  if(currentStep !==1){\n    return (\n      <a href=\".\" className=\"waves-effect waves-light btn pink\" onClick={this._prev}>Previous</a>\n    )\n  }\n  return null;\n}\n\nget nextButton(){\n  let currentStep = this.state.currentStep;\n  if(currentStep <2){\n    return (\n      <a href=\".\" className=\"waves-effect waves-light btn pink\" onClick={this._next}>Next</a>      \n    )\n  }\n  return null;\n}\n    render() {  \n      M.AutoInit(); \n      return (\n        <React.Fragment>\n        <h1>Create a Bracket</h1>\n          \n        <form onSubmit={this.handleSubmit}>\n          <Step1 \n            currentStep={this.state.currentStep} \n            handleChange={this.handleChange}\n            duration={this.state.duration}\n            options={this.state.options}\n            title={this.state.title} \n            display={this.state.display}\n          />\n          <Step2 \n            currentStep={this.state.currentStep} \n            handleChange={this.handleChange}\n            options={this.state.options}\n          />\n          {this.previousButton}\n          {this.nextButton}\n        </form>\n        </React.Fragment>\n      )\n      }\n  }","/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/Vote.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/VoteSubmitted.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/FourOFour.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/Welcome.js",[],"/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/Step2.js",["73"],"import React, {Component} from 'react'\nimport './Step2.css'\n\nexport default class Step2 extends Component {\n\n    componentDidMount(){\n\n    } \n\n    render() {\n        console.log(this.props)\n      if (this.props.currentStep !== 2) { // Prop: The current step\n        return null\n      }\n      // The markup for the Step 1 UI\n      //get this.props.options\n      // multiply the array of div/input by that optionsnum/2\n      // set variable maybe for loops of options num and set that to the placeholder and name\n        let optionNum = parseInt(this.props.options)\n        let optionArray = Array.from({length: optionNum}, (_, i) => i + 1)\n     \n        //matchups empty array \n        //iterate thru optionsArray\n        //for each div, {optionArray[i]}\n       \n        function makeInput(optionNumber) {\n            return <input type=\"text\" placeholder={\"Option \" + optionNumber} name={\"Option \" + optionNumber} />\n        }\n\n        const matchups = []\n\n        for (let i = 1; i < optionNum; i+= 2){\n            let option1 = makeInput(i)\n            let option2 = makeInput(i +1)\n            let finalDiv =  <div className=\"bracket-options\">\n                    {option1}\n                    <br></br>\n                    VS\n                    <br></br>\n                    {option2}\n                </div>\n            \n            matchups.push(finalDiv)\n        }\n\n        \n\n      return(\n        <div className=\"brackets\">\n            {matchups}\n        </div>\n      )\n    }\n  }","/Users/Simone/SEI-1019/unit_four/project4/tierBreaker_front/src/components/Step1.js",[],{"ruleId":"74","replacedBy":"75"},{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","severity":1,"message":"79","line":96,"column":17,"nodeType":"80","endLine":96,"endColumn":87},{"ruleId":"78","severity":1,"message":"79","line":108,"column":17,"nodeType":"80","endLine":108,"endColumn":87},{"ruleId":"78","severity":1,"message":"81","line":48,"column":17,"nodeType":"80","endLine":48,"endColumn":104},"no-native-reassign",["82"],"no-negated-in-lhs",["83"],"jsx-a11y/anchor-is-valid","The href attribute is required for an anchor to be keyboard accessible. Provide a valid, navigable address as the href value. If you cannot provide an href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","JSXOpeningElement","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a button and change it with appropriate styles. Learn more: https://github.com/evcohen/eslint-plugin-jsx-a11y/blob/master/docs/rules/anchor-is-valid.md","no-global-assign","no-unsafe-negation"]